{
	"name": "clashgg-sniper",
	"version": "1.2.0",
	"description": "This is a simple script that will automatically purchase items from the Clash.gg store as soon as they become available, if the price is below a certain threshold.",
	"main": "index.js",
	"scripts": {
		"start": "npx pm2 start",
		"logs": "npx pm2 logs \"Clash.gg Bot\"",
		"stop": "npx pm2 stop \"Clash.gg Bot\"",
		"restart": "npx pm2 restart \"Clash.gg Bot\" --update-env",
		"json2csv": "node src/tools/json2csv.js",
		"start-node": "node src/index.js"
	},
	"keywords": [
		"clash",
		"clash.gg",
		"clashgg",
		"sniper",
		"clash sniper",
		"clash.gg sniper"
	],
	"author": "Easton S.",
	"license": "ISC",
	"dependencies": {
		"axios": "^1.4.0",
		"dotenv": "^16.3.1",
		"json2csv": "^6.0.0-alpha.2",
		"mongoose": "^7.3.4",
		"pm2": "^5.3.0",
		"puppeteer": "^20.9.0",
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-stealth": "^2.11.2",
		"puppeteer-extra-plugin-user-preferences": "^2.4.1",
		"steam-totp": "^2.1.2",
		"steam-tradeoffer-manager": "^2.10.6",
		"steam-user": "^5.0.2",
		"steamcommunity": "^3.47.0",
		"winston": "^3.10.0",
		"ws": "^8.13.0"
	},
	"devDependencies": {
		"eslint": "^8.45.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^16.0.1",
		"eslint-plugin-promise": "^6.1.1"
	}
}
